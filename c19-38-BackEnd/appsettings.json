{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "ValidateIssuerSigningKey": true,
    "IssuerSigningKey": "insertSecretKey",
    "ValidateIssuer": true,
    "ValidIssuer": "https://localhost:7281",
    "ValidateAudience": true,
    "ValidAudience": "front-url",
    "RequiredExpirationTime": false,
    "ValidateLifeTime": false
  }
}
